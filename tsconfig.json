{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "types": [
      "node",
      "@docusaurus/module-type-aliases"
    ],
    "jsx": "react",
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "noEmit": true,
    "incremental": true,
    "skipLibCheck": true,
  },
}